<div class="panel panel-default">
  <div class="panel-heading" id="painel_list"><div class="text-center"><h1>Listing Monitorings</h1></div></div>
    <div id="painel_body" class="paine-body">
      <table class="table table-hover">
            <thead>
              <tr>
                <th>
                   <span class="glyphicon glyphicon-map-marker" aria-hidden="true"></span>
                     Local
                </th>
                <th>
                   <span class="glyphicon glyphicon-bullhorn" aria-hidden="true"></span>
                   Description
                </th>
                <th colspan="7"></th>
              </tr>
            </thead>
          
      
            <tbody>
              <% @monitorings.each do |monitoring| %>
                <tr>
                  <td><%= monitoring.local %></td>
                  <td><%= monitoring.description %></td>
                  
                </tr>
              <% end %>
            </tbody>
      </table>
    </div>
  </div>
</div>
<br>


