<div class="panel panel-default">
  <div class="panel-heading" id="painel_list"><div class="text-center"><h1>Listing Monitorings</h1></div></div>
    <div id="painel_body" class="paine-body">
      <table class="table table-hover">
            <thead>
              <tr>
                <th>
                   <span class="glyphicon glyphicon-map-marker" aria-hidden="true"></span>
                     Local
                </th>
                <th>
                   <span class="glyphicon glyphicon-bullhorn" aria-hidden="true"></span>
                   Description
                </th>
                <th colspan="7"></th>
              </tr>
            </thead>
          
      
            <tbody>
              <% @monitorings.each do |monitoring| %>
                <tr>
                  <td><%= monitoring.local %></td>
                  <td><%= monitoring.description %></td>
                  <td><%= link_to 'Show', monitoring %>
                    <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span>
                  </td>
                  <td><%= link_to 'Edit', edit_monitoring_path(monitoring) %>
                    <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
                  </td>
                  <td><%= link_to 'Destroy', monitoring, method: :delete, data: { confirm: 'Are you sure?' } %>
                    <span class="glyphicon glyphicon-trash" aria-hidden="true"></span>
                  </td>
                </tr>
              <% end %>
            </tbody>
      </table>
    </div>
  </div>
</div>
<br>

<%= link_to 'New Monitoring', new_monitoring_path %>
